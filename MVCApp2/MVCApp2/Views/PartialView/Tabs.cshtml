@{
	Layout = null;
	ViewData["Title"] = "Tabs Pages";
}
<!-- 1. BOOTSTRAP v4.0.0         CSS !-->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<!-- 2. GOOGLE JQUERY JS v3.2.1  JS !-->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<!-- 3. BOOTSTRAP v4.0.0         JS !-->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
<div>
	<!--Tab Buttons -->
	<ul id="tabstrip" class="nav nav-tabs" role="tablist">
		<li class="active">
			<a href="#_FirstTab" role="tab" data-toggle="tab" aria-selected="true">Submission</a>|
		</li>
		<li>
			<a href="#_SecondTab" role="tab" data-toggle="tab" aria-selected="false">Search</a>|
		</li>
		<li>
			<a href="#_ThirdTab" role="tab" data-toggle="tab" aria-selected="false">Comments</a>
		</li>
	</ul>

	<div class="tab-content">
		<div class="tab-pane fade" id="_FirstTab">
			<h3>Score</h3>
			<partial name="Score" />
		</div>
		<div class="tab-pane fade" id="_SecondTab">
			<h3>Cars</h3>

			<partial name="ChildList" />
		</div>
		<div class="tab-pane fade" id="_ThirdTab">
			<h3>Tom</h3>
			<partial name="ChildViewData" view-data="ViewData" />
		</div>
	</div>
</div>
